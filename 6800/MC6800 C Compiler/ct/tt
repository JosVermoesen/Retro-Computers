 a=1+2;
 b=-1;
 c=10+100-20+a+20-10;
 c=a+(3+2)+b;
 a=-(10+20+30);
 1+2+3+4;
 a=2*3+4*5;
 while ((c=getchar()) != -1)
  switch (c) {
  case '0':
  case '1':
  case '6':
  case '9':
   ndigit[c-'0']++;
   break;
  case ' ':
  case '\n':
  case '\t':
   nwhite++;
