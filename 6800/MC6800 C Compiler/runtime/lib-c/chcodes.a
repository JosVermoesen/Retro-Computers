 fcb WSPACE|CNTL  <nl>
 fcb CNTL  <vt>
 fcb WSPACE|CNTL  <ff>
 fcb WSPACE|CNTL  <cr>
 fcb CNTL  <so>
 fcb CNTL  <si>
*
* Characters 0x10 through 0x1F
*
 fcb CNTL  <dle>
 fcb CNTL  <dc1>
 fcb CNTL  <dc2>
 fcb CNTL  <dc3>
 fcb CNTL  <dc4>
 fcb CNTL  <nak>
 fcb CNTL  <syn>
 fcb CNTL  <etb>
*
 fcb CNTL  <can>
 fcb CNTL  <em>
 fcb CNTL  <sub>
 fcb CNTL  <esc>
 fcb CNTL  <fs>
 fcb CNTL  <gs>
 fcb CNTL  <rs>
 fcb CNTL  <us>
*
* Characters 0x20 through 0x2F
*
 fcb WSPACE|PRINT  ' '
 fcb PRINT  '!'
 fcb PRINT  '"'
 fcb PRINT  '#'
 fcb PRINT  '$'
 fcb PRINT  '%'
 fcb PRINT  '&'
 fcb PRINT  '''
*
 fcb PRINT  '('
 fcb PRINT  ')'
 fcb PRINT  '*'
 fcb PRINT  '+'
 fcb PRINT  ','
 fcb PRINT  '-'
 fcb PRINT  '.'
 fcb PRINT  '/'
*
* Characters 0x30 through 0x3F
*
 fcb HEX|DEC  '0'
 fcb HEX|DEC  '1'
 fcb HEX|DEC  '2'
 fcb HEX|DEC  '3'
 fcb HEX|DEC  '4'
 fcb HEX|DEC  '5'
 fcb HEX|DEC  '6'
 fcb HEX|DEC  '7'
*
 fcb HEX|DEC  '8'
 fcb HEX|DEC  '9'
 fcb PRINT  ':'
