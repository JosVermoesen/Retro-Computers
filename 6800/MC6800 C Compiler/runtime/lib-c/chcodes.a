 fcb HEX|UPPER  'B'
 fcb HEX|UPPER  'C'
 fcb HEX|UPPER  'D'
 fcb HEX|UPPER  'E'
 fcb HEX|UPPER  'F'
 fcb UPPER  'G'
*
 fcb UPPER  'H'
 fcb UPPER  'I'
 fcb UPPER  'J'
 fcb UPPER  'K'
 fcb UPPER  'L'
 fcb UPPER  'M'
