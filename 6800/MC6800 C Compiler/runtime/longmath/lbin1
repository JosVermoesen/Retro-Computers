* routines for doing long math operations

 name Long_Binary1


* temp storage cells

 global lret,dtemp

  bss
lret rmb 2
dtemp rmb 2
  text

* push long

  global pshlong
pshlong std dtemp
  puls d
  std lret
  ldd 2,x
  pshs d
  ldd 0,x
  pshs d
  ldd dtemp
  jmp [lret]

* add long values

  global addlong
addlong pshs d
  ldd 2,s
  std lret
  ldd 6,s
  addd 10,s
  std 10,s
  ldd 4,s
  adcb 9,s
  adca 8,s
  std 8,s
  puls d
  leas 6,s
  jmp [lret]

* assign long value

  global asnlong
asnlong pshs d
  ldd 4,s
  std 0,x
  ldd 6,s
  std 2,x
  puls d,pc

