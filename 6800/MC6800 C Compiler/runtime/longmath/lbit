* long bitwise routines

 name Long_Bit

 text

  global andlong
andlong pshs d
  ldd 4,s
  anda 8,s
  andb 9,s
  std 8,s
  ldd 6,s
  anda 10,s
  andb 11,s
  std 10,s
  puls d,x
  leas 4,s
  jmp 0,x

* bitwise or long values

  global borlong
borlong pshs d
  ldd 4,s
  ora 8,s
  orb 9,s
  std 8,s
  ldd 6,s
  ora 10,s
  orb 11,s
  std 10,s
  puls d,x
  leas 4,s
  jmp 0,x

* bitwise exclusive or long values

  global xorlong
xorlong pshs d
  ldd 4,s
  eora 8,s
  eorb 9,s
  std 8,s
  ldd 6,s
  eora 10,s
  eorb 11,s
  std 10,s
  puls d,x
  leas 4,s
  jmp 0,x

