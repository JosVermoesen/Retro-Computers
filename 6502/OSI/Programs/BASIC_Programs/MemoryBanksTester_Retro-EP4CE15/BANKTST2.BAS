 100 REM TEST BANKED MEMORY SPACES
 110 REM MEMORY BANK HAS 4KB FROM $C000-$DFFF (49152-57343 DEC)
 120 PRINT "TEST RAM BANK"
 130 S1=49152:REM START OF SRAM BANK
 140 E1=57343:REM END OF SRAM BANK
 150 B1=65492:REM BANK SELECT REGISTER $FFD4
 170 C1=64:REM NUMBER OF 8K BANKS
 175 PRINT "BANK REGISTER TEST - ";
 180 FOR BK=0 TO C1-1
 182 POKE B1,BK
 184 IF PEEK(B1)<>BK GOTO 194
 186 NEXT BK
 188 PRINT "PASSED"
 190 GOTO 200
 192 PRINT "FAILED":GOTO 1000
 200 REM WRITE BANK NUMBER TO FIRST LOCATION IN EACH BANK
 210 PRINT "TEST FIRST LOCATION OF EACH BANK - ";
 220 FOR BK=0 TO C1-1
 230 POKE B1,BK:POKE S1,BK
 240 NEXT BK
 250 REM READ BACK VALUES AND CHECK
 260 FOR BK=0 TO C1-1
 270 POKE B1,BK
 275 RD=PEEK(S1)
 280 IF RD=BK GOTO 320
 290 PRINT "FAILED "
 300 PRINT "  BANK =";BK
 310 PRINT "  VALUE =";RD:GOTO 1000
 320 NEXT BK
 330 PRINT "PASSED"
 400 PRINT "TESTING 8K MEMORY, FILLING"
 410 TV=0:POKE B1,0
 420 FOR AD=S1 TO E1
 430 POKE AD,TV
 440 TV=TV+1
 450 TV=TV AND 255
 460 IF TV=0 THEN PRINT "W";
 470 NEXT AD
 500 PRINT:PRINT "FILL COMPLETE, READING"
 510 TV=0
 520 FOR AD=S1 TO E1
 530 IF PEEK(AD)=TV GOTO 570
 540 PRINT "4K VALUE BANK 1 TEST FAILED - ADDRESS ";AD;
 550 PRINT "VALUE ";PEEK(AD)
 560 GOTO 1000
 570 TV=TV+1
 580 TV=TV AND 255
 590 IF TV=0 THEN PRINT "R";
 600 NEXT AD
 610 PRINT:PRINT "4K VALUE BANK 1 TEST PASSED"
 1000 REM
 